        <font>font30</font>
        <textcolor>FFFFFFFF</textcolor>
        <shadowcolor>FF000000</shadowcolor>
        <shadowoffsetx>2</shadowoffsetx>
        <shadowoffsety>2</shadowoffsety>
        <align>left</align>
      </control>

      <!-- Current time label -->
      <control type="label" id="5001">
        <left>20</left>
        <top>70</top>
