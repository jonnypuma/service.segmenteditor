          <control type="label">
            <left>10</left>
            <top>5</top>
            <width>1120</width>
            <height>25</height>
            <label>Segment $INFO[ListItem.Property(segment_num)] - $INFO[ListItem.Property(label)] - $INFO[ListItem.Property(start_hms)] to $INFO[ListItem.Property(end_hms)]</label>
            <font>font14</font>
            <textcolor>FFFFFFFF</textcolor>
            <align>left</align>
            <visible>String.IsEqual(ListItem.Property(segment_type),normal)</visible>
